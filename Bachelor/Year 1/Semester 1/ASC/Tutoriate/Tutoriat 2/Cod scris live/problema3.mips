.data
    n:.space 4
    sp:.asciiz " "
.text
main:

    li $v0, 5
    syscall
    move $t0, $v0

    sw $t0, n

    li $t1, 0
    li $t3, 0

    loop:

        beq $t1, $t0, exit

        li $v0, 5
        syscall
        move $t2, $v0

        div $t2, $t2, 3
        sub $t2, $t2, 2

        add $t3, $t3, $t2

        addi $t1, $t1, 1
        j loop

exit:

    li $v0, 1
    move $a0, $t3
    syscall

    li $v0, 10
    syscall