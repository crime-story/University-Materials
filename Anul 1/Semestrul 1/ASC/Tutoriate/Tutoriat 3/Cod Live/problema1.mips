.data

    v:.word 5, 19, 25, 13, 8
    n:.word 5
    sp:.asciiz " "

.text
main:

    lw $t0, n

    li $t1, 0
    la $t2, v

    loop:

        beq $t0, $t1, exit

        lw $a0, 0($t2)
        li $v0, 1
        syscall

        la $a0, sp
        li $v0, 4
        syscall

        addi $t1, $t1, 1
        addi $t2, $t2, 4

        j loop

exit:

    li $v0, 10
    syscall