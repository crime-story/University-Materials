.data
    v:.space 28             # 7 elemente, adică 28 de bytes
    n:.word 7
.text
main:

    lw $t0, n
    li $t1, 0
    li $t2, 0

    loop:
        beq $t0, $t1, exit

        li $v0, 5
        syscall
        sw $v0, v($t2)      # salvează valoarea citită in vectorul v
                            # la poziția $t2

        addi $t1, $t1, 1
        addi $t2, $t2, 4

        j loop

    exit:

    li $v0, 10
    syscall

