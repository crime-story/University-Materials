.data
    n:.space 4
    sp:.asciiz " "
.text
main:

    li $v0, 5
    syscall
    move $t0, $v0

    sw $t0, n

    li $t1, 0

    loop:

        beq $t1, $t0, exit

        li $v0, 1
        move $a0, $t1
        syscall

        li $v0, 4
        la $a0, sp
        syscall

        addi $t1, $t1, 1
        j loop

exit:

    li $v0, 10
    syscall